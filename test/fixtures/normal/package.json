{
  "spm": {
    "scripts": {
      "prebuild": "less",
      "postbuild": "csso"
    },
    "buildArgs": "--global jquery:jQuery",
    "build": {
      "global": {
        "react": "React"
      },
      "babel": true
    },
    "uglify": {
      "sourceMap": true
    },
    "server": {
      "devtool": "eval"
    },
    "pathmap": ["^src/(.*)", "$1"],
    "registry": "http://private.spm"
  }
}
